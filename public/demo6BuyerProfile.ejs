        $('#card_holder_name').val('');
        $('#card_number').val('');
        $('#expire_month').val('');
        $('#expire_year').val('');
        $('#cvc').val('');
    });
    $('#closeaddBankDetails').click(function(){
        $('#routing_number').val('');
        $('#account_number').val('');
        $('#account_holder_name').val('');
        $('#account_holder_type').val(0);
    });
