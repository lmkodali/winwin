<!DOCTYPE html>
<html>
<head>
  <title>Win Win</title>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
  <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">
  <style type="text/css">
    .tbl_left{
      padding-left: 18px !important;
  }
    .txt-mod{
      text-align: center;color:#FFF;font-size: 22px;
    }
    .fl-right{
      float: right;
    }
    .pad-spc{
      padding-top: 11px;
    }
    .no-bor-radius{
      border-radius: 0 !important;
    }
    .badge {
      display: inline-block;
      min-width: 10px;
      padding: 10px 40px !important;
      font-size: 16px !important;
      font-weight: 700;
      line-height: 1;
      color: #FFF !important;
      text-align: center;
      white-space: nowrap;
      vertical-align: middle;
      background-color: #337ab7 !important;
      border-radius: 10px;
    }
    .navbar{
      min-height: 60px !important;
    }
    .nav-tabs,.tab-content,.div-verify-btn{
      margin: 0 20px;
    }
    .btn-group-justified,.table-bordered,.div-profile,.div-password,.div-invite,{
      margin: 20px 0;
    }
    .pad-top{margin:12px 0 ;}
    .div-verify-btn{
        margin-top: 20px;
    }
    .verify{
        width:150px;
    }
    .required{color:red;}
    .topspc{
        margin-top: 10px;
    }
    .error{color:red;}
    .pad-left{padding-left: 0px !important; }
    .col_sm_12{width:100%;}
    .col_sm_4{width:33%;float:left;}
    .text-center-red{color:red;}
    .fb_share{padding-left:160px}
    .err{color:red;}
    .frmbtntopspc{margin-top:18px;}
    .bg{margin-left : 20px; margin-right : 20px ; background-color:#286090}
    .liActive{color : white;}
    .liActive:hover{color:rgba(255, 255, 255, 0.54)}
    a:hover{text-decoration: none}
  </style>
</head>
<body>
  <nav class="navbar navbar-inverse no-bor-radius">
    <div class="container-fluid pad-spc">
      <div class="col-sm-4">
        <div class="navbar-header txt-mod">
          Win Win
        </div>
      </div>
      <div class="col-sm-4 txt-mod">
        <span class="badge"></span>
      </div>
      <div class="col-sm-4">
        <div class="navbar-header txt-mod fl-right">
          <!-- <button type="button" class="btn btn-danger" >Log Out</button> -->
          <span style="font-size:12px;margin-right:15px;">Hi,<%= name%></span>
          <a href="/api/products/demo6AdminLogout" class="btn btn-danger">Log Out</a>
        </div>
      </div>
    </div>
  </nav>
    <!-- <h1 class="page-heading">Buyers</h1> --><!-- End page heading -->
    <!-- Begin breadcrumb -->
    <ol class="breadcrumb info bg">
      <li>
        <a href="/api/products/demo6BuyersList" class="liActive"><i class="fa fa-home"></i></a>
      </li>
      <li>
        <a href="/api/products/demo6BuyersList" class="liActive">View Sellers</a>
      </li>
      <li class="active" style="color:rgba(255, 255, 255, 0.54)">Add Seller</li>
    </ol>
    <!-- End Bread crumb -->
    <ul class="nav nav-tabs">
      <li class=""><a data-toggle="tab"  onclick="window.location.href='/api/products/demo6AdminList'">Administrators</a></li> 
      <li class=""><a data-toggle="tab"  onclick="window.location.href='/api/products/demo6BuyersList'">Buyers</a></li> 
      <li class="active"><a data-toggle="tab" id="sellers">Sellers</a></li> 
      <li class=""><a data-toggle="tab" id="recentTransaction" onclick="window.location.href='/api/products/demo6AdminListTransactions'">Transactions</a></li> 

    </ul>
  <div class="container-fluid">
      <div class="tab-content">
        <div id="administrators">
        </div>
        <div id="buyers" class="tab-pane fade">
        </div><!-- buyers -->
        <div id="sellers">
          <!-- Begin basic form elements -->
          <form action="/api/products/demo6AddSeller" method="post" id="frm_add_seller" enctype="multipart/form-data">
            <div class="the-box">
              <div class="col-sm-12 pad-top">
                <div class="col-sm-4">
                    <label>Name</label>
                    <input type="text" name="name" id="name" class="form-control" value=""  placeholder="Your Name">
                    <span class="err" value="" id="errName"></span>
                </div>
                <div class="col-sm-4">
                    <label>Email</label>
                    <input type="text" name="email" id="email" class="form-control" value="" placeholder="Your Email">
                    <span class="err" value="" id="errEmail"></span>
                </div>
                <div class="col-sm-4">
                    <label>Mobile</label>
                    <input type="text" name="phone" id="phone" class="form-control" value="" placeholder="Your Mobile">
                    <span class="err" value="" id="errMobile"></span>
                </div>
              </div>  
              <div class="col-sm-12 pad-top">
                <div class="col-sm-4">
                    <label>Address</label>
                    <textarea rows="3" name="address" id="address" class="form-control">
                    </textarea>
                    <span class="err" value="" id="errAddress"></span>
                </div>
                <div class="col-sm-4">
                    <label>Country</label>
                    <input type="text" name="country" id="country" class="form-control" value="" placeholder="Your Country">
                    <span class="err" value="" id="errCountry"></span>
                </div>
                <div class="col-sm-4">
                    <label>City</label>
                    <input type="text" name="city" id="city" class="form-control" value="" placeholder="Your City">
                    <span class="err" value="" id="errCity"></span>
                </div>
            </div>
            <div class="col-sm-12 pad-top">
              <div class="col-sm-4">
                  <label>Zip Code</label>
                  <input type="text"  name="zip_code" id="zip_code" class="form-control" value="" placeholder="Zip code">
                  <span class="err" value="" id="errZipcode"></span>
              </div>
              <div class="col-sm-4">
                  <label>Photo ID/ SSN / Driver License</label>
                  <input type="text" name="unique_id" id="unique_id" class="form-control" 
                  value="" placeholder="Your photoId/ SSN / DriverLicense">
                  <span class="err" value="" id="errUniqueid"></span>
              </div>
              <div class="col-sm-4">
                  <label>Education or Employer Details</label>
                  <input type="text" class="form-control" name="edu_emp" id="edu_emp" value="" placeholder="Your Education / Employee Detail">
                  <span class="err" value="" id="errEduEmp"></span>
              </div>  
            </div>    
            <div class="col-sm-12 pad-top">
              <div class="col-sm-4">
                  <label>Status</label>
                   <select class="form-control" tabindex="2" name="status" id="status">
                    <option value="">--Select--</option>
                    <option value="Active">Activate</option>                  
                    <option value="Inactive">Deactivate</option>
                  </select>
                  <span class="err" id="errStatus"></span>
              </div>
              <div class="col-sm-4">
                <label>Password</label>
                <input type="password" class="form-control" id="password" name='password' placeholder="Enter password">
                <span class="error" id="errPassword"></span>                  
              </div>
              <div class="col-sm-4">
                <label>Confirm Password</label>
                <input type="password" class="form-control " id="cpassword" name='cpassword' placeholder="Enter Confirm password">
                <span class="error" id="errCpassword"></span>
              </div>            
            </div>
            <div class="col-sm-12">
              <div class="col-sm-4">
                <div class="form-group">
                  <button class="btn btn-success active frmbtn frmbtntopspc" type='button' id="saveSeller">Save Changes</button>
                  <!-- <a class="btn btn-danger active frmbtn" href='cancel()'>Cancel</a> -->
                  <button  type="button"  class="btn btn-danger frmbtn frmbtntopspc" onclick="window.location.href='/api/products/demo6SellersList'">Cancel</button>
                </div>
              </div>
            </div>
            </div><!-- /.the-box -->
          </form>
          <!-- End basic form elements -->
        </div>        
        <div id="recentTransaction">
        </div>
      </div>
  </div><!-- /.container-fluid -->
  <script type="text/javascript">
    $(document).ready(function() {
       $("#msg_display_area").fadeOut(3000);
    });    
    $('#saveSeller').click(function(){
      var status_flag =  name_flag = email_flag = mobile_flag =address_flag = country_flag = city_flag = zip_code_flag = unique_flag = edu_emp_flag =password_flag = cpassword_flag= 0 ;
      var status = $('#status option:selected').val();
      var name = $('#name').val().trim();
      var email = $('#email').val().trim();
      var mobile = $('#phone').val().trim();
      var address = $('#address').val().trim();
      var country = $('#country').val().trim();
      var  city = $('#city').val().trim();
      var zip_code = $('#zip_code').val().trim();
      var unique_id = $('#unique_id').val().trim();
      var edu_emp = $('#edu_emp').val().trim(); 
      var password = $('#password').val();
      var cpassword = $('#cpassword').val();
      var password_length=$("#password").val().length;
      if(!name){
        name_flag = 0;
          $("#errName").html("Please enter name.");
      }
      else{
        $("#errName").html("");
        name_flag = 1; 
      }
      if(!email){
        email_flag = 0;
          $("#errEmail").html("Please enter email.");
      }
      else{
        $("#errEmail").html("");
        email_flag = 1; 
      }
      if(!mobile){
        mobile_flag = 0;
          $("#errMobile").html("Please enter mobile.");
      }
      else{
        $("#errMobile").html("");
        mobile_flag = 1; 
      }
      if(!address){
        address_flag = 0;
          $("#errAddress").html("Please enter address.");
      }
      else{
        $("#errAddress").html("");
        address_flag = 1; 
      }
      if(!country){
        country_flag = 0;
          $("#errCountry").html("Please enter Country.");
      }
      else{
        $("#errCountry").html("");
        country_flag = 1; 
      }
      if(!city){
        city_flag = 0;
          $("#errCity").html("Please enter city.");
      }
      else{
        $("#errCity").html("");
        city_flag = 1; 
      }
      if(!zip_code){
        zip_code_flag = 0;
          $("#errZipcode").html("Please enter zip code.");
      }
      else{
        $("#errZipcode").html("");
        zip_code_flag = 1; 
      }
      if(!unique_id){
        unique_flag = 0;
          $("#errUniqueid").html("Please enter photoid / ssn / license no.");
      }
      else{
        $("#errUniqueid").html("");
        unique_flag = 1; 
      }
      if(!edu_emp){
        edu_emp_flag = 0;
          $("#errEduEmp").html("Please enter education / employee details.");
      }
      else{
        $("#errEduEmp").html("");
        edu_emp_flag = 1; 
      }
      if(!status){
        status_flag = 0;
          $("#errStatus").html("Please select status.");
      }
      else{
        $("#errStatus").html("");
        status_flag = 1; 
      }
      
      if(password==""){
        $('#errPassword').html("Password required.");
        password_flag=0;
      }
      else{
        $('#errPassword').html("");
        if(password_length < 8 || password_length > 15 ){
          $('#errPassword').html("Password length between 8-15.");
          password_flag=0;
        }
        else{
          $('#errPassword').html("");
          password_flag=1;
        } 
      } 
      if(cpassword==""){
        $('#errCpassword').html("Confirm password required.");
        cpassword_flag=0;
      }
      else{
        $('#errCpassword').html("");
        if(password!=cpassword){
          $('#errCpassword').html("Password & Confirm password must be same.");   
          cpassword_flag=0;         
        }
        else{
          $('#errCpassword').html("");
          cpassword_flag=1;
        }
      } 
      if(status_flag ==0 || name_flag ==0 || email_flag ==0 || mobile_flag ==0 ||address_flag ==0 || country_flag ==0 || city_flag ==0 || zip_code_flag ==0 || unique_flag ==0 || edu_emp_flag ==0 ||password_flag==0 || cpassword_flag==0 ){
        return false;  
      }
      else{
        $('#frm_add_seller').submit();
      }
    });
  </script>

</body>
</html>
   

</body>
</html>